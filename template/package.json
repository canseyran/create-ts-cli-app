{
  "name": "template",
  "version": "1.0.0",
  "description": "",
  "main": "./src/main.ts",
  "scripts": {
    "start": "tsx ./src/main.ts",
    "build:script": "esbuild ./src/main.ts --format=iife --bundle --platform=node --banner:js='#!/usr/bin/env node' --outfile=./dist/app",
    "build:js": "esbuild ./src/main.ts --format=iife --bundle --platform=node  --outfile=./dist/app.js",
    "dev": "tsx watch ./src/main.ts",
    "test": "vitest run",
    "test:watch": "vitest .",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
