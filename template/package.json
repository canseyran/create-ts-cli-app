  },
  "devDependencies": {
    "@types/node": "^20.14.8",
    "esbuild": "^0.21.5",
    "@eslint/js": "^9.5.0",
    "@types/inquirer": "^9.0.7",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "3.3.2",
    "tsx": "^4.15.7",
    "typescript-eslint": "^7.14.1",
    "vitest": "^1.6.0"
  },
  "overrides": {
    "eslint": "^9.5.0"
  },
  "type": "module"
